#include <iostream>
using namespace std;
#include<vector>
#include<cmath>
#include<string>

int main()
{
    int n;
    cin >> n;

    int k;

    vector<int> v = {};

    for (int i = 0; i < n; i++)
    {
        cin >> k;
        v.push_back(k);
    }

    for (int i = n - 1; i >= 0; i--)
    {
        cout << v[i]<< ' ' ;
    }
}
